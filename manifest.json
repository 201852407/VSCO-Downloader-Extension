{
    "manifest_version": 2,
    "name": "VSCO Post downloader",
    "description": "Saves the contents of the selected VSCO post.",
    "author": "Raleigh Littles",
    "homepage_url": "https://github.com/raleighlittles/VSCO-Downloader-Extension",
    "version": "0.1",
    "content_scripts": [
      {
        "matches": [
          "https://vsco.co/*"
        ],
        "js": ["content.js"]
      }
    ],
    "browser_action": {

    },
    "icons": { "48":  "icons/icon48.png",
              "128":  "icons/icon128.png"},
    "background": {
      "scripts": ["background.js"]
    },
    "permissions": [
      "storage",
      "downloads"
  ]
}
